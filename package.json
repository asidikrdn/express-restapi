{
  "name": "express-restapi",
  "version": "1.0.0",
  "description": "REST API using nodejs with expressjs framework",
  "main": "./src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate-up": "npx sequelize db:migrate",
    "migrate-down": "npx sequelize db:migrate:undo:all",
    "seed-up": "npx sequelize db:seed:all",
    "seed-down": "npx sequelize db:seed:undo:all",
    "start": "nodemon src/server.js"
  },
  "keywords": [
    "rest",
    "api"
  ],
  "author": "Ahmad Sidik Rudini",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
